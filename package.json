{
    "name": "react-boiler-plate",
    "version": "1.0.0",
    "description": "react boiler plate",
    "main": "index.js",
    "engine": {
        "node": "10.16.0",
        "npm": "6.9.0"
    },
    "scripts": {
        "start": "node dist/server/index.js",
        "backend": "ts-node-dev server/index.js",
        "frontend": "npm run start --prefix client",
        "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
        "build": "tsc -p tsconfig.json"
    },
    "author": "John ahn",
    "license": "ISC",
    "dependencies": {
        "@types/jest": "^26.0.22",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.18.3",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.5",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "http": "0.0.1-security",
        "http-proxy-middleware": "^1.0.6",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "mongoose": "^5.12.0",
        "multer": "^1.4.2",
        "react-redux": "^5.0.7",
        "saslprep": "^1.0.3",
        "socket.io": "^4.0.0",
        "supports-color": "^7.1.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.36",
        "concurrently": "^4.1.0",
        "nodemon": "^1.19.1",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.3"
    },
    "repository": {
        "type": "git",
